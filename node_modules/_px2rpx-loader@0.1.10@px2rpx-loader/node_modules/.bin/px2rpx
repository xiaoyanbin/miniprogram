#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_px2rpx@0.5.3@px2rpx/bin/px2rpx.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_px2rpx@0.5.3@px2rpx/bin/px2rpx.js" "$@"
  ret=$?
fi
exit $ret
